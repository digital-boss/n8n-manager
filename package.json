{
  "name": "@digital-boss/n8n-manager",
  "version": "0.1.1",
  "description": "",
  "main": "esbuild.js",
  "bin": {
    "8man": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "ts-types": "tsc --emitDeclarationOnly --outDir dist",
    "create-exec": "(echo '#!/usr/bin/env node'; cat dist/index.js) > dist/cli.js && chmod +x dist/cli.js",
    "build": "rimraf dist && node esbuild.js && npm run ts-types && npm run create-exec",
    "exec": "npm run build && node dist/index.js",
    "genver": "genversion -s -e src/version.ts",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.7.23",
    "esbuild": "^0.15.10",
    "genversion": "^3.1.1",
    "jest": "^29.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "commander": "^9.4.1",
    "https-proxy-agent": "^5.0.1",
    "lodash.merge": "^4.6.2",
    "node-fetch": "^3.2.10",
    "tunnel": "^0.0.6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
